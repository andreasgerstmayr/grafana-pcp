{
    "extends": [
        "@grafana/eslint-config"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/no-floating-promises": "error",
        "no-restricted-globals": [
            "error",
            "name",
            "close",
            "history",
            "length",
            "open",
            "parent",
            "scroll",
            "stop",
            "event"
        ],
        "no-console": [
            "error",
            {}
        ]
    }
}
